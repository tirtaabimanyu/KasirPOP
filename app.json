{
  "expo": {
    "name": "kasirbodoh",
    "slug": "kasirbodoh",
    "version": "1.0.0",
    "orientation": "landscape",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "jsEngine": "hermes",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#6750A4"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "userInterfaceStyle": "light",
      "bundleIdentifier": "com.tirtaabimanyu.kasirbodoh",
      "infoPlist": {
        "NSBluetoothAlwaysUsageDescription": "This app need a bluetooth connection to discover bluetooth printers."
      }
    },
    "android": {
      "userInterfaceStyle": "light",
      "permissions": ["android.permission.BLUETOOTH_ADMIN"],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#6750A4"
      },
      "package": "com.tirtaabimanyu.kasirbodoh"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "f1c75e90-5121-4694-8278-326a29e48c8c"
      }
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 23,
            "compileSdkVersion": 31,
            "targetSdkVersion": 31,
            "buildToolsVersion": "31.0.0"
          },
          "ios": {
            "deploymentTarget": "13.0"
          }
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you choose suitable picture for your products."
        }
      ],
      ["./plugins/withDisableForcedDarkModeAndroid.js", {}]
    ]
  }
}
